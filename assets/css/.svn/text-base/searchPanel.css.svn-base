#fixed-window{
	width: 390px;
	height: 807px;
	background: #0071E1;
	z-index: 99;
	position: fixed;
	left: 20px;
	top:20px;
}
#fixed-window .layui-tab{
	width: 370px;
	margin:10px auto 0;
}
#fixed-window .layui-tab .layui-tab-title{
	height: 30px;
}
.layui-tab-title li{
	padding:0;
	width: 186px;
	height: 29px;
	line-height: 13px;
	border-radius: 5px 5px 0px 0px;
	/*border-bottom: 1px solid red;*/
}
.layui-tab-title li.layui-this{
	/*border-bottom: 1px solid #1789ff;*/
}
.areas-title li{
	position: relative;
	border-bottom:solid 1px #e1e7ed;
	color: #b2b8be;
}
.areas-title li .lines{
	width: 48px;
	height: 2px;
	background-color: transparent;
	position: absolute;
	left: 68px;
	bottom: -1px;
	z-index: 99;
}
.areas-title li.layui-this .lines{
	background-color: #1789ff;
}
.layui-tab-title .layui-this:after{
	height: 31px;
	border:0;
}
.layui-tab-title li:nth-child(3){
	margin-right: 0;
}
.layui-tab-content{
	padding: 0;
}
.float-list li.layui-nav-item{
	border: solid 1px #e1e7ed;
	margin-bottom: 5px;
}
.float-list li.layui-nav-item:nth-child(1){
	margin-top:15px;
}
.layui-nav-tree .layui-nav-bar{
	background: transparent;
}
.layui-nav .layui-nav-more {
    content: '';
    width: 0;
    height: 0;
    border-style: solid dashed dashed;
    border-color: #000000 transparent transparent;
    overflow: hidden;
    cursor: pointer;
    transition: all .2s;
    -webkit-transition: all .2s;
    position: absolute;
    top: 22px;
    right: 10px;
    border-width: 6px;
    border-top-color: #000000;
 }
 .layui-nav .layui-nav-item a{
 	color: #000000;
 }
 .layui-nav-itemed>a, .layui-nav-tree .layui-nav-title a, .layui-nav-tree .layui-nav-title a{
 	color: #000!important;
 }
 .layui-nav-itemed>a, .layui-nav-tree .layui-nav-title a, .layui-nav-tree .layui-nav-title a:hover{
 	color: #000!important;
 }
 .layui-nav-tree .layui-nav-item a:hover{
 	background: #FFFFFF;
 	color: #000;
 }
.layui-nav .layui-nav-mored,.layui-nav-itemed .layui-nav-more {
	top: 12px;
	right: 10px;
	border-style: dashed dashed solid;
	border-color: transparent transparent #000
}
.float-list .layui-nav-item .layui-nav-child{
	width: 368px;
	height: 330px;
	box-sizing: border-box;
	border-top: solid 1px #e1e7ed;
}
.layui-nav-itemed .layui-nav-child{
	background: #FFFFFF !important;
}
.content-nav{
	width: 350px;
	height: 420px;
}
.layui-table-cell{
	color: #000000;
	text-align: center;
}
/*权限分配*/
.element-query{
	width: 370px;
	position: relative;
	margin-top:10px;
}
.colu-title{
	width: 30px;
	height: calc(100% - 21px);
	overflow: hidden;
}
.colu-title li{
	padding: 0;
	min-width: 25px;
	width: 30px;
	height: 92px;
	float: clear;
	display: block;
	margin-bottom: 5px;
	line-height: 18px;
	padding-top:8px;
	border-radius: 5px 0px 0px 5px;
}
.colu-title li br{
	margin:0;
	padding:0;
}
.colu-content{
	width: 343px;
	height: 100%;
	box-sizing: border-box;
	border: solid 1px #e1e7ed;
	overflow: hidden;
	margin-left: -1px;
	position: absolute;
	left: 30px;
	top:0;
}
.colu-title li{
	border: solid 1px #e1e7ed;
}
.colu-title li.layui-this{
	background-color: #1789ff;
	color:#fff;
}
.layui-laypage .layui-laypage-curr .layui-laypage-em{
	background: #1789ff;
}
.layui-tab-title{
	border-bottom-width:0;
}
.search-box>input {
	width: 248px;
	height: 38px;
	border-radius: 0 0 0 0;
	border: solid 1px #e1e7ed;
	float: left;
}

.search-box>button {
	background: url(../images/search.png) #1789ff 19px 4px no-repeat;
	width: 70px;
	height: 38px;
	border-radius: 0 0 0 0;
	float: left;
	margin:0;
	padding: 0;
}

.layui-tab-content.colu-content select {
	width: 245px;
	height: 30px;
	border: solid 1px #e1e7ed;
	margin-top: 10px;
	background: #f0f4f8;
}
